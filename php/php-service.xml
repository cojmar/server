<service>
	<id>php</id>
	<name>php</name>
	<description>php</description>
	<env name="PHP_FCGI_MAX_REQUESTS" value="0" />
	<executable>F:\Server\php\php-cgi.exe</executable>
	<startargument>-c</startargument>
	<startargument>F:\Server\php\php.ini</startargument>
	<startargument>-b</startargument>
	<startargument>127.0.0.1:9000</startargument>
	<onfailure action="restart" />
	<logpath>F:\Server\php\logs</logpath>
	<logmode>roll</logmode>
</service>